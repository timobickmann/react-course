{
	"name": "et358-vite-react-site",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "run-p api frontend",
		"frontend": "vite --port 3020 --open",
		"api": "json-server --watch ./src/data/db.json --port 3021",
		"build": "tsc && vite build",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
